<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:defaultValue="5"
        android:entries="@array/number_of_pass_icons"
        android:entryValues="@array/number_of_pass_icons"
        android:key="no_of_pass_icons"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="Total Pass Icons"/>

    <Preference
        android:key="choose_pass_icon"
        android:title="Choose your Pass Icons"/>

    <Preference
        android:key="custom_pass_icon"
        android:title="Upload your own Pass Icon"/>

    <Preference
        android:key="view_pass_icons"
        android:title="View Your Pass Icon"/>

</PreferenceScreen>

